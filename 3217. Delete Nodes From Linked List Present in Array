/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode() : val(0), next(nullptr) {}
 *     ListNode(int x) : val(x), next(nullptr) {}
 *     ListNode(int x, ListNode *next) : val(x), next(next) {}
 * };
 */
class Solution {
public:
    ListNode* modifiedList(vector<int>& nums, ListNode* head) {
        int n=nums.size();
        unordered_map<int,int> m;

        for(int i=0;i<n;i++){
            m[nums[i]]++;
        }
        if(head->next==NULL && m.find(head->val)!=m.end()) return NULL;
        if(head->next==NULL && m.find(head->val)==m.end()) return head;
        ListNode* temp=head->next;
        ListNode* prev=head;
        while(temp!=NULL){
            if(m.find(temp->val)!=m.end()){
                ListNode* temp2=temp->next;
                prev->next=temp2;
                temp=temp2;
            }
            else{
                prev=temp;
                temp=temp->next;
            }
        }
        if(m.find(head->val)!=m.end()) return head->next;
        return head;
    }
};
